

reset

set terminal epslatex size 12.5cm,7.5cm color colortext

set logscale x 2
set log x
#set grid x
#set grid y
set format '$%g$'
set ylabel "$S_N$"
set xlabel "$N$"

#unset colorbox

#
# define line styles using explicit rgbcolor names
#
set style line 1 lt 2 lc rgb "red" lw 3
set style line 2 lt 2 lc rgb "green" lw 2
set style line 3 lt 2 lc rgb "orange" lw 2



set xrange [1000:100000000]
#set title "Problema di Basilea - Singola precisione"
set key center right
set output 'plot_yrmaxE.tex'

#show style line

plot 'plot_yRmaxE_0.dat' with linespoints ls 1 title '$L = 0$',\
     'plot_yRmaxE_1.dat' with linespoints ls 2 title '$L = 1$',\
     'plot_yRmaxE_2.dat' with linespoints ls 3 title '$L = 2'



